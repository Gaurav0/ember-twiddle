{{#paper-menu position='target-right target' as |menu|}}
  {{#menu.trigger class='layout-column'}}
    {{#paper-button class='md-menu-origin'}}
      Ember
      {{paper-icon 'arrow drop up'}}
    {{/paper-button}}
  {{/menu.trigger}}
  {{#menu.content width=2 as |content|}}
    {{#each versions as |version| }}
      {{#content.menu-item class='md-menu-align-target' onClick=(action 'versionSelected' 'ember' version)}}
        {{version}}
      {{/content.menu-item}}
    {{/each}}
  {{/menu.content}}
{{/paper-menu}}

