{{#paper-menu as |menu|}}
  {{#menu.trigger class='layout-column'}}
    {{#paper-button}}
      Ember
    {{/paper-button}}
  {{/menu.trigger}}
  {{#menu.content width=4 as |content|}}
    {{#each versions as |version| }}
      {{#content.menu-item onClick=(action "versionSelected" "ember" version)}}
        {{version}}
      {{/content.menu-item}}
    {{/each}}
  {{/menu.content}}
{{/paper-menu}}

