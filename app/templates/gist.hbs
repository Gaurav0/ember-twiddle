<MainGist
  @model={{this.model}}
  @session={{this.session}}
  @numColumns={{this.numColumns}}
  @fullScreen={{this.fullScreen}}
  @openFiles={{this.openFiles}}
  @isSidebarOpen={{this.isSidebarOpen}}
  @onSidebarUpdated={{action (mut this.isSidebarOpen)}}
  @route={{this.route}}
  @applicationUrl={{this.applicationUrl}}
  @unsaved={{this.unsaved}}
  @isGistSaving={{this.isGistSaving}}
  @isRevision={{this.isRevision}}
  @saveGist={{route-action "saveGist"}}
  @transitionQueryParams={{action "transitionQueryParams"}}
  @titleUpdated={{route-action "titleUpdated"}}
/>
